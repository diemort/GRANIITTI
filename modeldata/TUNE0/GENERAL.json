// Model parameters ("TUNE0")
//
// mikael.mieskolainen@cern.ch, 2020
{
  
  // ----------------------------------------------------------------------
  // Global soft model parameters (eikonal screening, elastic, SD, DD)
  "PARAM_SOFT" : {

    // Pomeron trajectory
    "DELTA_P"  : 0.098,         // Intercept
    "ALPHA_P"  : 0.065,         // Bare slope, GeV^{-2} (+ pion loop insert)

    // Couplings
    "gN_P"     : 8.20,          // Pomeron-proton GeV^{-1}
    "gN_O"     : 0.10,          // Odderon-proton GeV^{-1} (ANSATZ!)
    "g3P"      : 0.15,          // Triple Pomeron coupling (N.B. normalized units g3P/gn_P) (for SD/DD/EXCITATION)
    "gamma"    : 0.45,          // Eikonal matrix coupling [0 ... 1] (CURRENTLY NOT USED)

    // Form factor parameters
    "fc1"      : 0.34,          // Proton form factor param 1 (GeV^2)
    "fc2"      : 5.0,           // Proton form factor param 2 (GeV^2)
    "fc3"      : 10.0,          // Pion loop form factor (GeV^2)

    // Odderon
    "ODDERON_ON" : false
  },

  // ----------------------------------------------------------------------
  // Jacob-Wick helicity amplitudes etc.
  "PARAM_SPIN" : {

    // 2 -> 1 generation (fusion) spin correlations active
    "SPINGEN" : true,

    // 1 -> 2 decay spin correlations active
    "SPINDEC" : true,

    // Spin polarization frame ("CS" = Collins-Soper, "CM" = Non-Rotated Center of Momentum, "HX" = Helicity)
    "FRAME" : "CS", // Default Collins-Soper

    // Maximum angular momentum value for sliding Pomeron helicity trajectories
    "JMAX" : 5,
  },

  // ----------------------------------------------------------------------
  // Parameters for flat (DEBUG/ANALYSIS) amplitudes
  "PARAM_FLAT" : {
    "b" : 6,                     // t-slope [GeV^{-2}], |M|^2 ~ exp(bt_1)exp(b_t2)
  },

  // ----------------------------------------------------------------------
  // Monopolium / Monopole pair process parameters
  // N.B. Monopole mass is read from PDG file
  "PARAM_MONOPOLE" : {

    // Coupling scheme: "beta-dirac", "dirac"
    "coupling" : "beta-dirac",

    // Dirac charge n = 1,2,3,4,...
    "gn" : 1,

    // Monopolium bound state energy level (>= 13) controlling bound state (negative) binding energy
    "En"  : 13,

    // Monopolium (MMbar) bare width (GeV)
    "Gamma0" : 10.0

  },

  // -----------------------------------------------------------------------
  // Regge amplitude parameters (used for central system)

  "PARAM_REGGE" : {

      // Continuum amplitude t/u channel form factor:
      // 0 = "exp", 1 = "orear", 2 = "pow" (Orear and powerlaw seems the best)
      //
      // Note that with (interfering) resonances, the continuum parameters
      // should be fitted together with the resonances.
      "offshellFF" : 2,

      // Form factor parameters
      "b_EXP" :   0.944,            // GeV^-2
      "a_OREAR" : 0.638,            // GeV^-1
      "b_OREAR" : 1.886,            // GeV^-1
      "b_POW" :   0.500,            // GeV^2

      // Regge-trajectory parameters (pomeron, f2, a2)
      "a0"  : [1.088, 0.55, 0.55],  // Intercept (alpha_0 = 1 + DELTA)
      "ap"  : [0.20, 0.93, 0.93],   // Slope (alpha') [GeV^{-2}]
      "sgn" : [1, -1, 1],           // Regge signature (sigma) (+-1)
      "s0"  : 1.0,                  // Normalization scale (GeV^2)

      // Intermediate off-shell meson M* propagator "Reggeization"
      // (NOT IMPLEMENTED CURRENTLY)
      "reggeize" : false,

      // Pomeron-Pomeron-Resonance vertex flux [1/m^2]^{lambda} parameter
      "omega" : 0.5,
  },

  // ---------------------------------------------------------------------
  // Continuum amplitude couplings (non-tensor Pomeron model):
  //
  // For coupling constants see e.g.
  //   pipi: Phys. Rev. D81 (2010) 036003
  //   KK:   Phys. Rev. D85 (2012) 014026
  //
  // Originating from fits by: A. Donnachie and P.V. Landshoff, Phys. Lett.B296 (1992) 227.
  // Note that these couplings are technically only compatible with the same Pomeron/Reggeon
  // linear trajectory parameters as used by Donnachie-Landshoff.
  //
  // Fixed final state couplings [in GeV^{-2}] (1 mb = 2.56819 GeV^{-2}]
  //
  // These DL-couplings are transformed inside the program by:
  // gpi_P = PARAM_REGGE::c[0] / PARAM_SOFT::gN_P

  // pion
  "PARAM_PI" : {
    "c" : [35.00184, 81.63672, 10.86264],
    "n" : [true, false, false],  // Which Reggeons are exchanged (pomeron, f2, a2)
  },

  // kaon
  "PARAM_K" : {
    "c" : [30.35376, 44.31084, 23.38164],
    "n" : [true, false, false]
  },

  // proton (ansatz) (here one can compare with gN_P above)
  "PARAM_P" : {
    "c" : [27.0, 0, 0],
    "n" : [true, false, false]
  },

  // rho(770) (ansatz)
  "PARAM_RHO" : {
    "c" : [25.0, 0, 0],
    "n" : [true, false, false]
  },

  // phi(1020) (ansatz)
  "PARAM_PHI" : {
    "c" : [18.0, 0, 0],
    "n" : [true, false, false]
  },

  // f0(500) (ansatz)
  "PARAM_F0500" : {
    "c" : [35.0, 0, 0],
    "n" : [true, false, false]
  },

  // other, such as lambda(1115) (ansatz)
  "PARAM_X" : {
    "c" : [15.0, 0, 0],
    "n" : [true, false, false]
  },


  // -----------------------------------------------------------------------
  // Proton structure functions and form factors

  "PARAM_STRUCTURE" : {

    // Proton structure functions (DL, CKMT)
    "F2" : "CKMT",

    // Coherent proton EM-form factors (DIPOLE, KELLY)
    "EM" : "KELLY",

    // QED coupling fixed scale (ZERO, MG) (in MadGraph yy -> X amplitudes)
    // ZERO means alpha(Q = 0 GeV) (Default)
    // MG   means MadGraph C++ default value (typically Q = Z mass)
    "QED_alpha" : "ZERO"
  },
  
  // -----------------------------------------------------------------------
  // Tensor Pomeron parameters (resonance parameters are under resonance files)
  "PARAM_TENSOR_POMERON" : {
    
    // Trajectory parameters
    "delta_P"   : 0.0808,
    "delta_O"   : 0.0808,
    "delta_1R"  : -0.5475,
    "delta_2R"  : -0.5475,
    
    "ap_P"      : 0.25,         // GeV^{-2}
    "ap_O"      : 0.25,         // GeV^{-2}
    "ap_1R"     : 0.9,          // GeV^{-2}
    "ap_2R"     : 0.9,          // GeV^{-2}
    "eta_O"     : 1.0,          // +- 1

    // Scales
    "M_O"       : 1.00,         // GeV
    "M_1R"      : 1.41,         // GeV
    
    // Couplings
    "gPNN"      : 1.87,         // GeV^{-1}, Pomeron-Proton-Proton
    "gPpipi"    : 1.76,         // GeV^{-1}, Pomeron-Pion-Pion
    "gPKK"      : 1.54,         // GeV^{-1}, Pomeron-Kaon-Kaon
    "gPrhorho"  : [0.70, 6.20], // [GeV^{-3}, GeV^{-1}], Pomeron-Rho-Rho
    "gPphiphi"  : [0.49, 4.27], // [GeV^{-3}, GeV^{-1}], Pomeron-Phi-Phi
    
    
    // QED
    "alpha_QED" : 0.007297352566354976, // alpha_QED at q^2 ~ 0
    "mu_ratio"  : 2.792847337,          // Proton magnetic moment in magneton units (mu_p / mu_N)
  },
  
  // -----------------------------------------------------------------------
  // Durham QCD parameters

  "PARAM_DURHAM_QCD" : {

    // Gluon loop momentum bounds (keep inside bounds set in NUMERICS.json)
    "qt2_MIN" : 0.4,   // GeV^2
    "qt2_MAX" : 100.0, // GeV^2

    // Gluon skewed PDF scale choise scheme [MIN (default), MAX, IN, EX, AVG]
    "PDF_scale" : "MIN",

    // Alpha_s scale choise (Q^2 = M^2/value, default = 1.0, other common 4.0)
    "alphas_scale" : 1.0,

    // Technical parameters: Gluon loop numerical discretization (keep a factor of 3)
    "N_phi" : 36,
    "N_qt"  : 24,

    // Angular |cos(theta*)| < MAXCOS cutoff for meson amplitudes [0,1]
    "MAXCOS" : 0.9,
  },

  // -----------------------------------------------------------------------
  // Good-Walker proton excitation parametrization
  // (IMPLEMENTATION UNDER CONSTRUCTION, NOT ACTIVE)

  "PARAM_NSTAR" : {

    // Relative (N*, N**, N***) couplings (are normalized to one)
    "rc" : [0.5, 0.3, 0.2]
  },

}
